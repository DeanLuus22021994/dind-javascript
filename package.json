{
  "name": "dind-javascript",
  "version": "1.0.0",
  "description": "Enhanced Docker-in-Docker JavaScript Environment",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:silent": "cross-env NODE_ENV=test jest --silent --verbose=false",
    "test:quiet": "cross-env NODE_ENV=test jest --silent --verbose=false --reporters=summary",
    "test:minimal": "cross-env NODE_ENV=test jest --silent --verbose=false --reporters=basic",
    "test:watch": "cross-env NODE_ENV=test jest --watch --silent",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --silent",
    "lint": "eslint --no-config-lookup --config .eslintrc.js src/ __tests__/",
    "lint:fix": "eslint --no-config-lookup --config .eslintrc.js src/ __tests__/ --fix",
    "docker:build": "docker build -t dind-javascript .",
    "docker:run": "docker run -p 3000:3000 dind-javascript",
    "docs:generate": "node scripts/generate-docs.js"
  },
  "keywords": [
    "docker",
    "javascript",
    "api",
    "express",
    "dind"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@graphql-tools/schema": "^10.0.1",
    "@openapi-contrib/openapi-schema-to-json-schema": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.0.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "nodemailer": "^6.9.10",
    "prom-client": "^15.1.3",
    "redis": "^4.6.7",
    "socket.io": "^4.7.5",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "mongodb-memory-server": "^9.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "rimraf": ">=5.0.0",
    "glob": ">=10.3.10",
    "lodash.isequal": false,
    "lodash.get": false,
    "superagent": false
  }
}
