{
  "name": "dind-javascript",
  "version": "1.0.0",
  "description": "Enhanced Docker-in-Docker JavaScript Environment",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:silent": "cross-env NODE_ENV=test JEST_SILENT=true jest --silent",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:windows": "set NODE_ENV=test&& jest",
    "lint": "eslint src/ __tests__/",
    "lint:fix": "eslint src/ __tests__/ --fix",
    "docker:build": "docker build -t dind-javascript .",
    "docker:run": "docker run -p 3000:3000 dind-javascript",
    "docs:generate": "node scripts/generate-docs.js",
    "test:unix": "NODE_ENV=test jest",
    "test:win": "set NODE_ENV=test&& jest",
    "test:cross": "cross-env NODE_ENV=test jest"
  },
  "keywords": [
    "docker",
    "javascript",
    "api",
    "express",
    "dind"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "express-validator": "^7.0.1",
    "winston": "^3.10.0",
    "mongoose": "^7.4.0",
    "redis": "^4.6.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "apollo-server-express": "^3.12.1",
    "graphql": "^16.7.1",
    "@graphql-tools/schema": "^10.0.0",
    "prom-client": "^14.2.0",
    "socket.io": "^4.7.2",
    "nodemailer": "^6.9.4"
  },
  "devDependencies": {
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.45.0",
    "mongodb-memory-server": "^8.14.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
